ugh choices", "themes": [{"id": 22, "name": "Historical"}, {"id": 31, "name": "Drama"}, {"id": 32, "name": "Non-fiction"}, {"id": 39, "name": "Warfare"}], "url": "https://www.igdb.com/games/my-child-lebensborn", "videos": [{"id": 17912, "name": "Trailer", "video_id": "eKslwxILevs", "url": "https://www.youtube.com/watch?v=eKslwxILevs", "title": "Early trailer for the game \"My Child: Lebensborn\", now on Kickstarter", "thumb": "https://i.ytimg.com/vi/eKslwxILevs/maxresdefault.jpg"}]}