u0430\u043c\u0435\u043d\u043d\u044b\u0435 \u0441\u0442\u0432\u043e\u0440\u043a\u0438\u2026 \n\n\u0418\u0437 \u0437\u0430\u0431\u0432\u0435\u043d\u0438\u044f \u0432\u043e\u0441\u0441\u0442\u0430\u043b\u043e \u0434\u0440\u0435\u0432\u043d\u0435\u0435 \u0437\u043b\u043e \u2013 \u0438 \u043c\u0435\u0440\u0442\u0432\u044b\u0435 \u0432\u0441\u0442\u0430\u043b\u0438 \u043f\u0440\u043e\u0442\u0438\u0432 \u0436\u0438\u0432\u044b\u0445. \u0411\u0435\u0441\u0441\u0442\u0440\u0430\u0448\u043d\u044b \u0431\u044b\u043b\u0438 \u0432\u043e\u0438\u0442\u0435\u043b\u0438 \u0433\u043d\u043e\u043c\u043e\u0432, \u0437\u0430\u043a\u043e\u0432\u0430\u043d\u043d\u044b\u0435 \u0432 \u0441\u0430\u043c\u044b\u0435 \u043a\u0440\u0435\u043f\u043a\u0438\u0435 \u0434\u043e\u0441\u043f\u0435\u0445\u0438, \u043d\u043e \u0438\u0445 \u0445\u0440\u0430\u0431\u0440\u043e\u0441\u0442\u044c \u0431\u044b\u043b\u0430 \u0431\u0435\u0441\u0441\u0438\u043b\u044c\u043d\u0430 \u043f\u0440\u043e\u0442\u0438\u0432 \u0434\u0440\u0435\u0432\u043d\u0435\u0439 \u043c\u0430\u0433\u0438\u0438, \u0438 \u0431\u044b\u043b\u0430 \u0441\u043b\u0430\u0431\u0430 \u0438\u0445 \u0441\u0442\u0430\u043b\u044c \u043f\u0440\u043e\u0442\u0438\u0432 \u043f\u043b\u043e\u0442\u0438 \u0436\u0438\u0432\u044b\u0445 \u043c\u0435\u0440\u0442\u0432\u0435\u0446\u043e\u0432. \u041b\u0438\u0448\u044c \u043e\u0434\u043d\u043e \u0438\u043c \u043e\u0441\u0442\u0430\u043b\u043e\u0441\u044c \u2013 \u043e\u0442\u0441\u0442\u0443\u043f\u0438\u0442\u044c, \u0431\u0440\u043e\u0441\u0430\u044f \u0441\u0432\u043e\u0438 \u043f\u043e\u0434\u0437\u0435\u043c\u043d\u044b\u0435 \u043a\u0443\u0437\u043d\u0438\u0446\u044b \u0438 \u0434\u043e\u043c\u0430, \u0438 \u0438\u0441\u043a\u0430\u0442\u044c \u043f\u043e\u043c\u043e\u0449\u0438 \u043d\u0430\u0432\u0435\u0440\u0445\u0443, \u0432 \u0441\u043e\u043b\u043d\u0435\u0447\u043d\u044b\u0445 \u0433\u043e\u0440\u043d\u044b\u0445 \u0434\u043e\u043b\u0438\u043d\u0430\u0445.\n\n\u0415\u0433\u043e \u0437\u0432\u0430\u043b\u0438 \u041e\u0440\u0442\u0435\u0433\u043e\u0439, \u0438 \u043e\u043d \u043d\u0435 \u0443\u0434\u0438\u0432\u0438\u043b\u0441\u044f, \u0443\u0432\u0438\u0434\u0435\u0432 \u0443 \u0441\u0432\u043e\u0435\u0439 \u0445\u0438\u0436\u0438\u043d\u044b \u043f\u043e\u0441\u043b\u0430\u043d\u0446\u0435\u0432 \u0433\u043d\u043e\u043c\u043e\u0432 \u2013 \u044d\u0442\u043e \u0441\u043a\u0440\u043e\u043c\u043d\u043e\u0435 \u043e\u0431\u0438\u0442\u0430\u043b\u0438\u0449\u0435 \u043e\u0442\u0448\u0435\u043b\u044c\u043d\u0438\u043a\u0430 \u043f\u043e\u0441\u0435\u0449\u0430\u043b\u0438 \u0438 \u043a\u0443\u0434\u0430 \u0431\u043e\u043b\u0435\u0435 \u043d\u0435\u043e\u0431\u044b\u0447\u043d\u044b\u0435 \u0433\u043e\u0441\u0442\u0438. \u041e\u043d \u0441\u043b\u0443\u0448\u0430\u043b \u0438\u0445, \u0438 \u043f\u0435\u0440\u0435\u0434 \u0435\u0433\u043e \u0432\u043d\u0443\u0442\u0440\u0435\u043d\u043d\u0438\u043c \u0432\u0437\u043e\u0440\u043e\u043c \u043f\u0440\u043e\u043f\u043b\u044b\u0432\u0430\u043b\u0438 \u0432\u0438\u0434\u0435\u043d\u0438\u044f \u043f\u0440\u043e\u0448\u043b\u044b\u0445 \u0431\u0438\u0442\u0432, \u043f\u0438\u0440\u043e\u0432 \u0438 \u043f\u0440\u0435\u0434\u0430\u0442\u0435\u043b\u044c\u0441\u0442\u0432 \u2013 \u0435\u043c\u0443, \u0431\u044b\u0432\u0448\u0435\u043c\u0443 \u043f\u0440\u0438\u0434\u0432\u043e\u0440\u043d\u043e\u043c\u0443 \u043d\u0435\u043a\u0440\u043e\u043c\u0430\u043d\u0442\u0443, \u0431\u044b\u043b\u043e \u0447\u0442\u043e \u0432\u0441\u043f\u043e\u043c\u043d\u0438\u0442\u044c.\n\n\u041a\u043e\u0433\u0434\u0430-\u0442\u043e \u0417\u043b\u043e \u0443\u0436\u0435 \u0431\u044b\u043b\u043e \u043f\u043e\u0431\u0435\u0436\u0434\u0435\u043d\u043e, \u043d\u043e \u0441 \u0442\u0435\u0445 \u043f\u043e\u0440 \u043f\u0440\u043e\u0448\u043b\u043e \u043c\u043d\u043e\u0433\u043e \u0432\u0440\u0435\u043c\u0435\u043d\u0438. \u0414\u043b\u044f \u043d\u0435\u0433\u043e \u0441\u043d\u043e\u0432\u0430 \u043f\u0440\u0438\u0448\u043b\u043e \u0432\u0440\u0435\u043c\u044f \u0432\u0437\u044f\u0442\u044c\u0441\u044f \u0437\u0430 \u0431\u043e\u0435\u0432\u043e\u0439 \u043f\u043e\u0441\u043e\u0445 \u0438 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u044c \u0441\u0430\u043c\u044b\u0435 \u0441\u0438\u043b\u044c\u043d\u044b\u0435 \u0437\u0430\u043a\u043b\u0438\u043d\u0430\u043d\u0438\u044f - \u043c\u0438\u0440 \u043d\u0435 \u0434\u043e\u043b\u0436\u0435\u043d \u043f\u043e\u0433\u0440\u0443\u0437\u0438\u0442\u044c\u0441\u044f \u0432\u043e \u0442\u044c\u043c\u0443!\n\n\u041e\u0441\u043e\u0431\u0435\u043d\u043d\u043e\u0441\u0442\u0438 \u0438\u0433\u0440\u044b \u042d\u043f\u043e\u0445\u0430 \u0433\u0435\u0440\u043e\u0435\u0432 II: \u0423\u0436\u0430\u0441 \u0438\u0437 \u043f\u043e\u0434\u0437\u0435\u043c\u0435\u043b\u044c\u044f (Age of Heroes II: Underground Horror):\n\u2022 \u0423\u043b\u0443\u0447\u0448\u0435\u043d\u043d\u0430\u044f \u0438 \u043f\u0435\u0440\u0435\u0434\u0435\u043b\u0430\u043d\u043d\u0430\u044f \u0433\u0440\u0430\u0444\u0438\u043a\u0430, \u0434\u0432\u0430 \u043d\u043e\u0432\u044b\u0445 \u043c\u0438\u0440\u0430 \u2013 \u0433\u043e\u0440\u043d\u044b\u0435 \u0434\u043e\u043b\u0438\u043d\u044b \u0438 \u043f\u0435\u0449\u0435\u0440\u044b \u0433\u043d\u043e\u043c\u043e\u0432;\n\u2022 \u0412\u043e\u0437\u043c\u043e\u0436\u043d\u043e\u0441\u0442\u044c \u0441\u043e\u0445\u0440\u0430\u043d\u0435\u043d\u0438\u044f\n\u2022 10 \u043d\u043e\u0432\u044b\u0445 \u0441\u0443\u0449\u0435\u0441\u0442\u0432, \u043a\u0430\u043a \u0432 \u0432\u0430\u0448\u0435\u0439 \u0430\u0440\u043c\u0438\u0438, \u0442\u0430\u043a \u0438 \u0432 \u0430\u0440\u043c\u0438\u0438 \u0432\u0430\u0448\u0438\u0445 \u0432\u0440\u0430\u0433\u043e\u0432;\n\u2022 20 \u0443\u0440\u043e\u0432\u043d\u0435\u0439 \u0443\u043b\u0443\u0447\u0448\u0435\u043d\u0438\u044f \u0434\u043b\u044f \u0433\u0435\u0440\u043e\u044f, \u043c\u043d\u043e\u0436\u0435\u0441\u0442\u0432\u043e \u0432\u043b\u0438\u044f\u044e\u0449\u0438\u0445 \u043d\u0430 \u043f\u0430\u0440\u0430\u043c\u0435\u0442\u0440\u044b \u0430\u0440\u0442\u0435\u0444\u0430\u043a\u0442\u043e\u0432;\n\u2022 \u0422\u0440\u0438 \u0443\u0440\u043e\u0432\u043d\u044f \u043d\u043e\u0432\u043e\u0439, \u0440\u0430\u0437\u0440\u0443\u0448\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u0439 \u043c\u0430\u0433\u0438\u0438;\n\u2022 \u0412\u044b\u043f\u043e\u043b\u043d\u044f\u0442\u044c \u0441\u044e\u0436\u0435\u0442\u043d\u044b\u0435 \u0437\u0430\u0434\u0430\u043d\u0438\u044f, \u043e\u0431\u0449\u0430\u0442\u044c\u0441\u044f \u0441 \u043d\u043e\u0432\u044b\u043c\u0438 \u0441\u043e\u044e\u0437\u043d\u0438\u043a\u0430\u043c\u0438 \u0438 \u0441\u0440\u0430\u0436\u0430\u0442\u044c\u0441\u044f \u0441 \u043d\u043e\u0432\u044b\u043c\u0438 \u0432\u043e\u0439\u0441\u043a\u0430\u043c\u0438 \u043f\u0440\u043e\u0442\u0438\u0432 \u043d\u043e\u0432\u044b\u0445 \u043f\u0440\u043e\u0442\u0438\u0432\u043d\u0438\u043a\u043e\u0432;\n\u2022 \u0414\u0432\u0430 \u043c\u0438\u0440\u0430:\n\u0413\u043e\u0440\u043d\u0430\u044f \u0434\u043e\u043b\u0438\u043d\u0430\n\u041f\u043e\u0434\u0437\u0435\u043c\u0435\u043b\u044c\u044f \u0433\u043d\u043e\u043c\u043e\u0432\n\u2022 \u041f\u0440\u0435\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u0442\u044c \u0441\u0432\u043e\u0438\u0445 \u0432\u0440\u0430\u0433\u043e\u0432 \u043d\u0430 \u043f\u043e\u0432\u0435\u0440\u0445\u043d\u043e\u0441\u0442\u0438 \u0437\u0435\u043c\u043b\u0438 \u0438 \u0432 \u043f\u043e\u0434\u0437\u0435\u043c\u0435\u043b\u044c\u044f\u0445 \u0433\u043d\u043e\u043c\u043e\u0432, \u043f\u043b\u0430\u043d\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u0441\u0432\u043e\u044e \u0441\u0442\u0440\u0430\u0442\u0435\u0433\u0438\u044e, \u0441\u043e\u0431\u0438\u0440\u0430\u0442\u044c \u0432\u043e\u0439\u0441\u043a\u0430 \u0438 \u043f\u043e\u0431\u0435\u0436\u0434\u0430\u0442\u044c!\n\n\u0423\u043b\u0443\u0447\u0448\u0435\u043d\u0438\u044f \u043f\u043e \u0441\u0440\u0430\u0432\u043d\u0435\u043d\u0438\u044e \u0441 \u043f\u0435\u0440\u0432\u043e\u0439 \u0447\u0430